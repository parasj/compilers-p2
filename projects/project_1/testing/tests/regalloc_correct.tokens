let var u:id : unit ; var A:id , B:id , C:id , D:id , E:id : int := 0:intlit ; in A:id := A:id + 0:intlit ; B:id := B:id + 1:intlit ; C:id := C:id + 2:intlit ; D:id := D:id + 3:intlit ; E:id := E:id + 4:intlit ; if B:id <> 0:intlit then A:id := 0:intlit ; B:id := 1:intlit ; C:id := 2:intlit ; else D:id := 3:intlit ; E:id := 4:intlit ; endif ; u:id := printi:id ( A:id ) ; u:id := printi:id ( B:id ) ; u:id := printi:id ( C:id ) ; u:id := printi:id ( D:id ) ; u:id := printi:id ( E:id ) ; end